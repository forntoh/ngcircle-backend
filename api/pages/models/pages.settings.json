{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Pages",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "cta": {
      "type": "component",
      "repeatable": false,
      "component": "sections.cta",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 64,
      "minLength": 3,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "landing": {
      "type": "dynamiczone",
      "components": [
        "landing.primary-image",
        "shared.slider"
      ],
      "max": 1,
      "min": 1,
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "shared.markdown",
        "shared.media",
        "shared.media-desc",
        "shared.slider",
        "shared.spacer",
        "shared.network-group"
      ],
      "min": 1,
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
